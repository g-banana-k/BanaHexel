{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "windows": {
      "wix": {
        "language": "ja-JP"
      }
    },
    "publisher": "BananaHexagon",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "BanaHexel",
  "mainBinaryName": "BanaHexel",
  "version": "0.1.0",
  "identifier": "com.g-banana-k.banahexel",
  "plugins": {},
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "title": "BanaHexel",
        "width": 800,
        "height": 600,
        "decorations": false,
        "minWidth": 680,
        "minHeight": 360
      }
    ]
  }
}